{
  "name": "mm-preview",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run type-check",
    "format": "biome format --write",
    "format:imports": "biome check --write",
    "format:all": "biome check --write",
    "test": "turbo run test",
    "storybook": "turbo run storybook",
    "sdk:generate": "npm run generate:all --workspace=@mm-preview/sdk"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "turbo": "^2.3.3",
    "typescript": "^5"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "lightningcss": "^1.30.2"
  }
}
